{
  "root": true,
  "extends": ["plugin:react/recommended", "plugin:json/recommended"],
  "rules": {
    "plugin": ["react/recommended, json/recommended"],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "react/display-name": 0
  }
}
